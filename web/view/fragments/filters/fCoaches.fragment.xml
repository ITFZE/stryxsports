<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog title="Select Filter">
		<List items="{CoachFilter>/coachesFilterData}" mode="SingleSelectLeft"  includeItemInSelection="true" select="getViewonCoachs">
			<StandardListItem  title="{CoachFilter>txt_CoachesFilter}" iconInset="true"/>
		</List>
		<buttons>
			<Button id="confirmDialogBtn" visible="false" text="Confirm" press="onPressFilterConfirm"/>
			<Button text="Close" press="onPressDialogClose"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>